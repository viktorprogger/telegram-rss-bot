{
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "debril/feed-io": "^4.5",
        "guzzlehttp/guzzle": "^6.3",
        "monolog/monolog": "^2.0",
        "yiisoft/composer-config-plugin": "^1.0.0",
        "yiisoft/db": "^3.0@dev",
        "yiisoft/db-pgsql": "^3.0@dev",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/factory": "^3.0@dev",
        "yiisoft/validator": "^3.0@dev",
        "yiisoft/yii-console": "^3.0@dev",
        "yiisoft/yii-queue": "^3.0@dev",
        "yiisoft/yii-queue-amqp": "^3.0@dev"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,

    "autoload": {
        "psr-4": {
            "rssBot\\": "./"
        }
    },
    "extra": {
        "config-plugin": {
            "events-console": "config/events-console.php",
            "console": [
                "$common",
                "config/console.php"
            ],
            "params": [
                "$params",
                "config/params.php"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
