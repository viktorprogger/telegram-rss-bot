{
    "name": "viktorprogger/telegram-rss-bot",
    "description": "",
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "debril/feed-io": "^4.5",
        "guzzlehttp/guzzle": "^6.3",
        "yiisoft/cache": "^1.0",
        "yiisoft/cache-file": "^1.0",
        "yiisoft/config": "1.0.x-dev",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/factory": "^3.0@dev",
        "yiisoft/friendly-exception": "^1.0",
        "yiisoft/log": "^1.0",
        "yiisoft/log-target-file": "^1.0",
        "yiisoft/validator": "^3.0@dev",
        "yiisoft/yii-console": "^3.0@dev",
        "yiisoft/yii-cycle": "3.0.x-dev"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,

    "autoload": {
        "psr-4": {
            "rssBot\\": "./"
        }
    },
    "extra": {
        "config-plugin-options": { "force-check": true },
        "config-plugin": {
            "console": [
                "$common",
                "config/console.php"
            ],
            "params": [
                "$params",
                "config/params.php"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
