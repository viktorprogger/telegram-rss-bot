{
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",

        "guzzlehttp/guzzle": "^6.3",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/yii-console": "^3.0@dev",
        "yiisoft/factory": "^3.0@dev",
        "yiisoft/validator": "^3.0@dev",
        "debril/feed-io": "^4.5",
        "monolog/monolog": "^2.0",
        "yiisoft/yii-queue": "^3.0@dev",
        "yiisoft/yii-queue-amqp": "^3.0@dev",
        "yiisoft/db": "^3.0@dev",
        "yiisoft/db-pgsql": "^3.0@dev"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,

    "autoload": {
        "psr-4": {
            "rssBot\\": "./"
        }
    },
    "extra": {
        "config-plugin": {}
    },
    "config": {
        "sort-packages": true
    }
}
